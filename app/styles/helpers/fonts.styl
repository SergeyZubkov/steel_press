raleway = Raleway, sans-serif
roboto = Roboto, sans-serif


@font-face
	font-family Raleway
	src local("Raleway"),
			local("Raleway-local"),
			url('../fonts/Raleway-ExtraBold.ttf') 
	font-weight 900
	font-style normal

@font-face
	font-family Raleway
	src local("Raleway"),
			local("Raleway-local"),
			url('../fonts/Raleway-Regular.ttf') 
	font-weight 400
	font-style normal
			
@font-face
	font-family Roboto
	src local("Roboto"),
			local("Roboto-local"),
			url('../fonts/Roboto-Regular.ttf') 
	font-weight 400
	font-style normal
	
@font-face
	font-family Roboto
	src local("Roboto"),
			local("Roboto-local"),
			url('../fonts/Roboto-Light.ttf') 
	font-weight 300
	font-style normal
	
@font-face
	font-family Roboto
	src local("Roboto"),
			local("Roboto-local"),
			url('../fonts/Roboto-Bold.ttf') 
	font-weight 700
	font-style normal